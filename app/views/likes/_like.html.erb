<%= image_tag "star.png" ,class:"favorite-star-icon" ,width:"20",height:"20"%>
  <%= turbo_frame_tag dom_id(item, :like) do %>
  <% if user_signed_in? && item.liked_by?(current_user)%>
    <%=link_to "お気に入り済み", item_likes_path(item),  data: { turbo_method: :delete }, remote: true %>
  <% else %>
    <%=link_to "お気に入りする", item_likes_path(item), data: { turbo_method: :post }, remote: true %>
  <% end %>
<% end %>